  "private": true,
  "homepage": "http://gravity-ui.github.io/landing",
  "dependencies": {
    "@doc-tools/transform": "^2.16.0",
    "@gravity-ui/page-constructor": "^1.7.0",
    "@gravity-ui/uikit": "^3.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.3",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "bem-cn-lite": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.5.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@gravity-ui/eslint-config": "^1.0.2",
    "@gravity-ui/prettier-config": "^1.0.1",
    "@gravity-ui/stylelint-config": "^2.0.0",
    "@gravity-ui/tsconfig": "^1.0.0",
    "eslint": "^8.27.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "gh-pages": "^4.0.0",
    "nano-staged": "^0.8.0",
    "prettier": "^2.7.1",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "svg-inline-loader": "^0.8.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:styles": "stylelint '{styles,src}/**/*.scss'",
    "lint:prettier": "prettier --check '**/*.md'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nano-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
