  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE_BUNDLE=true next build",
    "start": "next start",
    "postbuild": "next-sitemap",
    "get-packages-versions": "node ./scripts/get-packages-versions.mjs",
    "prepare-metadata": "npm run get-packages-versions",
    "update-og-images": "node ./scripts/update-og-images.mjs",
    "update-packages-readme": "node ./scripts/update-packages-readme.mjs",
    "prestart": "npm run prepare-metadata",
    "prebuild": "npm run prepare-metadata",
    "pretest": "npm run prepare-metadata",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:styles": "stylelint '{styles,src}/**/*.scss'",
    "lint:prettier": "prettier --check '**/*.md'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier && npm run circular:check",
    "deploy": "echo 'gravity-ui.com' > ./out/CNAME && gh-pages -d build",
    "typecheck": "tsc --noEmit",
    "circular:check": "madge --circular --ts-config ./tsconfig.json src/",
    "client:rsd": "rsdoctor analyze --profile .next/.rsdoctor/manifest.json",
    "server:rsd": "rsdoctor analyze --profile .next/server/.rsdoctor/manifest.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
